{
    "WP": {
        "make": ["controller"],
        "listing": ["controllers","repositorys", "views", "templates", "routes", "dbmaps"],
        "map": ["up", "down"]
    },
    "CMD_REPLACE":{
        "controller": {
            "tag": ["name", "repository", "resource"],
            "replace": ["$this->Name", "$this->Template", "$this->Template"]
        },
        "repository": {
            "tag": ["name", "resource"]
        }
    }
}